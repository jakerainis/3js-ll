    "prettier": "^2.3.2",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup": "^2.36.2",
    "serve": "^11.3.2"
  },
  "dependencies": {
